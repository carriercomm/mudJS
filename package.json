  },
  "dependencies": {
    "body-parser": "^1.6.7",
    "cookie-parser": "~1.3.2",
    "debug": "^1.0.4",
    "express": "^4.8.8",
    "express-handlebars": "^1.1.0",
    "handlebars": "^2.0.0",
    "mongoose": "^3.8.16",
    "morgan": "^1.2.3",
    "serve-favicon": "^2.0.1",
    "socket.io": "^1.1.0"
  },
  "devDependencies": {
    "bower": "^1.3.9",
    "browserify": "^5.11.1",
    "browserify-shim": "^3.7.0",
    "grunt": "^0.4.5",
    "grunt-bower-task": "^0.4.0",
    "grunt-browserify": "^3.0.1",
    "grunt-concurrent": "^1.0.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-cssmin": "^0.10.0",
    "grunt-contrib-sass": "^0.8.1",
    "grunt-contrib-uglify": "^0.5.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-nodemon": "^0.3.0",
    "hbsfy": "^2.2.0",
    "load-grunt-tasks": "^0.6.0",
    "time-grunt": "^1.0.0"
  },
  "browser": {
    "jquery": "./client/requires/jquery/js/jquery.js",
    "backbone": "./client/requires/backbone/js/backbone.js",
    "backbone.marionette": "./client/requires/backbone.marionette/js/backbone.marionette.js",
    "underscore": "./client/requires/underscore/js/underscore.js",
    "socket": "./client/requires/socket.io-client/js/socket.io.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "underscore": {
      "exports": "_"
    },
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:$",
        "underscore:_"
      ]
    },
    "backbone.marionette": {
      "exports": "Marionette",
      "depends": [
        "jquery:$",
        "underscore:_",
        "backbone:Backbone"
      ]
    },
    "socket": {
      "exports": "socket"
    }
  }
}
